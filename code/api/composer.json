{
    "name": "franjid/subscribers-api",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^4.0",
        "symfony/http-foundation": "^7.0",
        "symfony/routing": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Project\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
